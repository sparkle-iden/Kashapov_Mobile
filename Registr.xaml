<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Kashapov.Registr"
             Title="Registr"
              NavigationPage.HasNavigationBar="False">
    <VerticalStackLayout Spacing="20">
        <Entry x:Name="Login_entry" Placeholder="Введите логин"></Entry>
        <Entry x:Name="Password_entry" Placeholder="Введите пароль"></Entry>
        <Button Text="Зарегестрироваться" x:Name="Registr_Button" Clicked="CounterBtn_Clicked" WidthRequest="250"></Button>
    </VerticalStackLayout>
</ContentPage>